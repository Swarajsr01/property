<?xml version="1.0" encoding="UTF-8" ?>
<odoo noupdate="1">
    <!--rent sequence data -->
    <record id="ir_sequence_rent" model="ir.sequence">
        <field name="name">Rent Sequence</field>
        <field name="code">rent.sequence</field>
        <field name="prefix">1R%(day)s</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>