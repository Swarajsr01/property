<odoo>
    <!--    template show when the user create rent order-->
    <template id="thank_you_page" name="Customer Form Successfully Submitted">
        <t t-call="website.layout">
            <div id="wrap">
                <div class="container">
                    <div style="margin-top:20px;margin-left:35%" class="col-md-12">
                        <div class="d-inline-block mx-auto p-4 text-center">
                            <i class="fa fa-paper-plane fa-2x mb-3 rounded-circle text-bg-success"
                               role="presentation"/>
                            <h1 class="fw-bolder">Thank You!</h1>
                            <p class="lead mb-0">Rent order is created successfully</p>
                            <a href="/property">Go to Homepage</a>
                        </div>
                    </div>
                </div>
            </div>
        </t>
    </template>
</odoo>
